<p class="field-data field-series">
{% for option in field.options() %}
    <input type="radio" name="{{ field.name }}" id="{{option.id}}"
           value="{{ option.value }}"
            {{ 'checked' if option.checked}} {{ 'disabled' if field.disabled }}
           class="form-control"
    >
    <label for="{{option.id}}"> {{ option.label}} </label>
{% endfor %}
</p>
